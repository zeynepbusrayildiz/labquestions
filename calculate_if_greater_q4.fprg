    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="A, B, C" type="Integer" array="False" size=""/>
            <output expression="&quot;Please enter the value of A:&quot;" newline="True"/>
            <input variable="A"/>
            <if expression="A&gt;50">
                <then>
                    <output expression="&quot;Please enter the value of B:&quot;" newline="True"/>
                    <input variable="B"/>
                    <if expression="B&gt;50">
                        <then>
                            <assign variable="C" expression="A+B"/>
