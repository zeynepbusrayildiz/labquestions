                        </then>
                        <else>
                            <output expression="&quot;Value of B must be greater than 50.&quot;" newline="True"/>
                        </else>
                    </if>
                </then>
                <else>
                    <output expression="&quot;Value of A must be greater than 50.&quot;" newline="True"/>
                </else>
            </if>
        </body>
    </function>
